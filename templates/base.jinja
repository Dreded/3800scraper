    <meta name="msapplication-config" content="/docs/4.5/assets/img/favicons/browserconfig.xml">
    <meta name="theme-color" content="#563d7c"> #}
    <!-- Custom styles for this template -->
    <link href="{{ url_for('static', filename='css/base.css') }}" rel="stylesheet">
    {% block css %}
    {% endblock %}
    {% endblock %}
  </head>
  <body>
  {% block navbar %}
    <nav class="navbar navbar-expand-md navbar-dark bg-dark fixed-top">
  <a class="navbar-brand" href="#">{{title}}</a>
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarsExampleDefault" aria-controls="navbarsExampleDefault" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>

  <div class="collapse navbar-collapse" id="navbarsExampleDefault">
    <ul class="navbar-nav mr-auto">
      {# <li class="nav-item active">
        <a class="nav-link" href="#">Home <span class="sr-only">(current)</span></a>
      </li> #}
    {% block navbaritems_AfterHome %}
    {% endblock %}
      {# <li class="nav-item">
        <a class="nav-link" href="#">Link</a>
      </li>
      <li class="nav-item">
        <a class="nav-link disabled" href="#" tabindex="-1" aria-disabled="true">Disabled</a>
      </li>
      <li class="nav-item dropdown">
        <a class="nav-link dropdown-toggle" href="#" id="dropdown01" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">Dropdown</a>
        <div class="dropdown-menu" aria-labelledby="dropdown01">
          <a class="dropdown-item" href="#">Action</a>
          <a class="dropdown-item" href="#">Another action</a>
          <a class="dropdown-item" href="#">Something else here</a>
        </div>
      </li> #}
      
    {% block navbaritems_End %}
    {% endblock %}
    </ul>
        <form method="post" class="form-inline my-2 my-lg-0">
            {% if audio == True %}
                <input type="hidden" name="audio" value='Disable_Audio'>
                <button type="submit" id=btn class="btn btn-primary my-2 my-sm-0" title="Disable Audio"><i class="fas fa-volume-up"></i></button>
            {% else %}
                <input type="hidden" name="audio" value='Enable_Audio'>
                <button type="submit" id=btn class="btn btn-primary my-2 my-sm-0" title="Enable Audio"><i class="fas fa-volume-mute"></i></button>
            {% endif %}
        </form>
    </form>
  </div>
</nav>
{% endblock %}

<main role="main" class="container">
  {% block main %}
  {% endblock %}

</main><!-- /.container -->

    <script>

document.addEventListener('DOMContentLoaded', function() {

    var url = 'http://127.0.0.1:5001/GUI-is-still-open'; 
    fetch(url, { mode: 'no-cors'});
    setInterval(function(){ fetch(url, { mode: 'no-cors'});}, 5000)();

});

</script>
